{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-metapipe"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"adf_sql_metadat_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'adf_sql_metadat'"
		},
		"controldb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'controldb'"
		},
		"sqdb_destination_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sqdb_destination'"
		},
		"sqldb_metadat_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sqldb_metadat'"
		},
		"sqldb_source_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sqldb_source'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adls1metapipe.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-metapipe_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"adf_sql_metadat_connectionString": {
						"value": "[parameters('adf_sql_metadat_connectionString')]"
					},
					"controldb_connectionString": {
						"value": "[parameters('controldb_connectionString')]"
					},
					"sqdb_destination_connectionString": {
						"value": "[parameters('sqdb_destination_connectionString')]"
					},
					"sqldb_metadat_connectionString": {
						"value": "[parameters('sqldb_metadat_connectionString')]"
					},
					"sqldb_source_connectionString": {
						"value": "[parameters('sqldb_source_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}